import java.util.Scanner;


public class DashBoard 
{
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for(int q=0;q<t;q++)
		{
			int n = sc.nextInt();
			int arr[] = new int[n];
			for(int i=0;i<n;i++)
			{
				arr[i] = sc.nextInt();
			}
			int min = arr[0];
			int max = arr[0];
			int maxpos, minpos;
			DashBoard db = new DashBoard();
			for(int j=0;j<arr.length-1;j++)
			{
				if(arr[j]%2==0);
				{
					maxpos = j;
					for(int k=j+1;k<arr.length;k++)
					{
						if(arr[k]%2==0)
						{
							if(arr[k]>max)
							{
								max = arr[k];
								maxpos = k;
							}
						}
						else
						{
							continue;
						}
					}
					db.swap(arr[j], arr[maxpos]);
				}
				
				else if(arr[j]%2==1)
				{
					minpos = j;
					for(int k=j+1;k<arr.length;k++)
					{
						if(arr[k]%2==1)
						{
							if(arr[k]<min)
							{
								min = arr[k];
								minpos = k;
							}
						}
						else
						{
							continue;
						}
					}
					db.swap(arr[j], arr[minpos]);
				}
				
			}
				
		}
	}
	
	void swap(int a, int b)
	{
		int temp = a;
		a = b;
		b = temp;
	}
}
