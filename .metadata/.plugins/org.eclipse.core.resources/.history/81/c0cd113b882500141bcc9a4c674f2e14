import java.util.*;
public class ReadPhoneNumber 
{
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int t =sc.nextInt();
		for(int e=0;e<t;e++)
		{
			String no = sc.next();
			String code = sc.next();
			int k=0;
			String[] pattern = code.split("-");
			int n = pattern.length;
			System.out.println(n);
			
			int i=0;
			for(i=0;i<n;i++)
			{
				
				for(int j=0;j<Integer.parseInt(pattern[i])-1;j++)
				{
					int count = 1;					
					while(no.charAt(k)==no.charAt(k+1) && k<no.length()-1)
					{
						k++;
						count++;
					}
					System.out.println("hi");
					 if(count==2)
						System.out.print("double");
					else if(count==3)
						System.out.print("triple");
					else if(count==4)
						System.out.print("quadruple");
					else if(count==5)
						System.out.print("quintuple");
					else if(count==6)
						System.out.print("sextuple");
					else if(count==7)
						System.out.print("septuple");
					else if(count==8)
						System.out.print("octuple");
					else if(count==9)
						System.out.print("nonuple");
					else if(count==10)
						System.out.print("decuple");
//					else if(count==11)
//						System.out.print();
					 
					if(code.charAt(k)=='0')
						System.out.print("zero");
					else if(code.charAt(k)=='1')
						System.out.print("one");
					else if(code.charAt(k)=='2')
						System.out.print("two");
					else if(code.charAt(k)=='3')
						System.out.print("three");
					else if(code.charAt(k)=='4')
						System.out.print("four");
					else if(code.charAt(k)=='5')
						System.out.print("five");
					else if(code.charAt(k)=='6')
						System.out.print("six");
					else if(code.charAt(k)=='7')
						System.out.print("seven");
					else if(code.charAt(k)=='8')
						System.out.print("eight");
					else if(code.charAt(k)=='9')
						System.out.print("nine");
					k++;
					
				}
				
			}
		}
	}
}
