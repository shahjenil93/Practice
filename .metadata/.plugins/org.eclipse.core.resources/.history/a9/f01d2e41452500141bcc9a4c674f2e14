import java.util.*;

public class BlackHorse
{
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for(int f=0;f<t;f++)
		{
			int n = sc.nextInt();
			int h;
			String[] g1 = new String[n];
			String[] g2 = new String[n];
			int i=0,j=0;
			for(h=0;h<n;h++)
			{
			
			
			String t1 = sc.next();
			String t2 = sc.next();
			BlackHorse bh = new BlackHorse();
			if((bh.isPresent(t1, g1)==false && bh.isPresent(t1, g1)==false ) && (bh.isPresent(t2, g2)==false && bh.isPresent(t2, g2)==false))
			{
				g1[i]=t1;
				i++;
				g2[j]=t2;
				j++;
			}
			else if((bh.isPresent(t1, g1)==true && bh.isPresent(t1, g2)==false ) || (bh.isPresent(t2, g1)==false && bh.isPresent(t2, g2)==true))
			{
				g1[i]=t1;
				i++;
				g2[j]=t2;
				j++;
			}
			else if((bh.isPresent(t1, g1)==false && bh.isPresent(t1, g2)==true ) || (bh.isPresent(t2, g1)==true && bh.isPresent(t2, g2)==false))
			{
				g1[i]=t2;
				i++;
				g2[j]=t1;
				j++;
			}
			else if(bh.isPresent(t1, g1)==true && bh.isPresent(t2, g2)==true)
			{
				break;
			}
		}
			if(h==n)
				System.out.println("YES");
			else
				System.out.println("NO");
		}
	}
	
	boolean isPresent(String t1, String g1[])
	{
		for(int i=0;i<g1.length;i++)
		{
			if(g1.equals(t1))
				return true;
		}
		return false;
				
	}
}
